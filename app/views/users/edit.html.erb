<h1>Edit User</h1>
<form method="post" action="/users/<%= @user.id %>">
  <input type="hidden" name="_method" value="patch" autocomplete="off">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <div>
    <label>Name </label>
    <input type="text" name="user[name]" value="<%= @user.name %>">
  </div>
  <div>
    <label>Email </label>
    <input type="text" name="user[width]" value="<%= @user.email %>">
  </div>
  <div>
    <label>Password </label>
    <input type="text" name="user[height]" value="<%= @user.password %>">
  </div>
  <div>
    <label>Bio </label>
    <input type="text" name="user[height]" value="<%= @user.bio %>">
  </div>
  <div>
    <label>DOB </label>
    <input type="text" name="user[height]" value="<%= @user.dob %>">
  </div>
  <div>
    <label>Phoen Number </label>
    <input type="text" name="user[height]" value="<%= @user.phone_number %>">
  </div>
  <div>
    <input type="submit" value="Update user">
  </div>
</form>